{
  "containerDefinitions": [
    {
      "name": "dev-archimedes-backend-container",
      "cpu": 500,
      "memory": 1024,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost:80/healthcheck/ || exit 1"
        ]
      }
    }
  ],
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "EC2"
  ]
}