{
  "family": "backendstackec2taskdefinition2E9226A20",
  "containerDefinitions": [
    {
      "name": "dev-archimedes-backend-container",
      "cpu": 500,
      "memory": 512,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "ACCESS_KEY",
          "value": "${ACCESS_KEY}"
        },
        {
          "name": "SECRET_KEY",
          "value": "${SECRET_KEY}"
        }
      ],
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost:80/healthcheck/ || exit 1"
        ]
      }
    }
  ],
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "EC2"
  ]
}